    "Employee": {
      "description": "Çalışan bilgileri. Hiyerarşik yapıda (ReportsTo ile)",
      "columns": {
        "EmployeeId": "Benzersiz çalışan kimliği (Primary Key)",
        "LastName": "Soyad",
        "FirstName": "Ad",
        "Title": "Ünvan",
