  "common_join_patterns": [
    {
      "description": "Sanatçı - Albüm - Parça ilişkisi",
      "query": "SELECT Artist.Name, Album.Title, Track.Name FROM Artist JOIN Album ON Artist.ArtistId = Album.ArtistId JOIN Track ON Album.AlbumId = Track.AlbumId"
    },
    {
      "description": "Müşteri - Fatura - Satış detayları",
      "query": "SELECT Customer.FirstName, Customer.LastName, Invoice.Total FROM Customer JOIN Invoice ON Customer.CustomerId = Invoice.CustomerId"
    },
    {
      "description": "En çok satan parçalar (satış miktarına göre)",
      "query": "SELECT Track.Name, SUM(InvoiceLine.Quantity) as TotalSold FROM Track JOIN InvoiceLine ON Track.TrackId = InvoiceLine.TrackId GROUP BY Track.TrackId ORDER BY TotalSold DESC"
    },
    {
      "description": "Türe göre satış analizi",
      "query": "SELECT Genre.Name, COUNT(InvoiceLine.InvoiceLineId) as Sales FROM Genre JOIN Track ON Genre.GenreId = Track.GenreId JOIN InvoiceLine ON Track.TrackId = InvoiceLine.TrackId GROUP BY Genre.GenreId"
    }
  ],
